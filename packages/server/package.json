{
  "name": "@wuct/server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "yarn run build && node build/server.js",
    "build": "rm -rf ./build && babel -s -x .ts -d ./build ./src",
    "watch:node": "nodemon --watch build/ build/server.js",
    "watch:js": "yarn run build -w",
    "watch": "yarn run build && concurrently \"yarn:watch:*\"",
    "type-check": "tsc"
  },
  "dependencies": {
    "graphql-import": "^0.7.1",
    "graphql-yoga": "^1.18.1",
    "graphql.macro": "^1.4.2",
    "source-map-support": "^0.5.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "babel-plugin-macros": "^2.6.1",
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1"
  }
}
